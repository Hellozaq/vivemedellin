type Review {
    id: String!
    userId: String!
    eventId: Int!
    rating: Int!
    comment: String
    createdAt: String!
    updatedAt: String
}

type Mutation {
    createReview(
        userId: String!
        eventId: Int!
        rating: Int!
        comment: String
    ): Review!
    deleteReview(reviewId: Int): Review!
}

type Query {
    allReviewsByEventId(eventId: Int!): [Review!]!
}
