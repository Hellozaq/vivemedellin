type Review {
    id: String!
    userId: String!
    eventId: Int!
    rating: Int!
    comment: String
    createdAt: String!
    updatedAt: String
}

input ReviewInput {
    userId: String!
    eventId: Int!
    rating: Int!
    comment: String
}

type Mutation {
    createReview(input: ReviewInput!): Review!
    deleteReview(reviewId: Int): String!
}

type Query {
    allReviewsByEventId(eventId: Int!): [Review!]!
}
